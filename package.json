{
  "name": "halo",
  "version": "0.8.3",
  "description": "Next-generation design system for AngelList and beyond.",
  "author": "Drew Lustro <drew@angel.co>",
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/halo.js",
  "module": "dist/halo.module.js",
  "style": "dist/halo.css",
  "types": "dist/typings/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist/* && gatsby clean",
    "prepare": "yarn build",
    "build": "yarn build:node",
    "build:node": "tsc && rollup -c rollup.config.babel.js",
    "changelog": "npx auto-changelog --package --commit-limit false  --template ./tools/changelog.compact.hbs",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "watch": "concurrently --kill-others --kill-others-on-fail --names \"HALO:STYLES,HALO:BUILD\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" --timestamp-format \"YYYY-MM-DD HH:MM\" \"yarn:watch:styles\" \"yarn:watch:build\"",
    "watch:build": "chokidar \"src/**/*.{ts,tsx,scss}\" --command \"yarn build\"",
    "watch:styles": "chokidar \"src/**/*.scss\" --command \"yarn run export:scss\"",
    "prepublishOnly": "yarn test && yarn lint",
    "test": "jest --config jest.config.js",
    "test:ci": "jest --config jest.config.js --runInBand",
    "test:watch": "jest --config jestconfig.json --watch --onlyChanged",
    "docs:build": "yarn docz:build",
    "docs:dev": "yarn docz:dev",
    "docz:build": "docz build",
    "docz:dev": "docz dev",
    "export:scss": "atsm -p 'src/**/*.scss'",
    "prettier:other:write": "prettier --write '**/*.{md,markdown}'",
    "prettier:other": "prettier '**/*.{md,markdown}' --list-different",
    "prettier:scripts:write": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "prettier:scripts": "prettier '**/*.{js,jsx,ts,tsx}' --list-different",
    "prettier:styles:write": "stylelint '**/*.scss' --fix && prettier --write '**/*.{css,scss}'",
    "prettier:styles": "stylelint '**/*.scss' && prettier '**/*.{css,scss}' --list-different",
    "prettier:write": "yarn run prettier:scripts:write && yarn run prettier:styles:write && yarn run prettier:other:write",
    "prettier": "yarn run prettier:scripts && yarn run prettier:styles && yarn run prettier:other",
    "release": "release-it",
    "eslint": "eslint '**/*.ts*' --format codeframe",
    "eslint:fix": "eslint '**/*.ts*' --format codeframe --fix",
    "tslint:fix": "tslint --fix --project . -t grouped -s node_modules/custom-tslint-formatters/formatters",
    "tslint:vscode": "tslint --project . -t vscode -s node_modules/custom-tslint-formatters/formatters",
    "tslint": "tslint --project . -t grouped -s node_modules/custom-tslint-formatters/formatters",
    "release-major": "release-it major",
    "release-minor": "release-it minor",
    "release-patch": "release-it patch",
    "typesync": "typesync"
  },
  "github": {
    "release": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/venturehacks/halo.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@types/acorn": "4.0.5",
    "@types/babel__core": "^7.1.3",
    "@types/classnames": "^2.2.9",
    "@types/concurrently": "^4.1.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/eslint": "6.1.3",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "^23.3.10",
    "@types/jscodeshift": "^0.6.3",
    "@types/lodash": "^4.14.139",
    "@types/node": "^11.13.7",
    "@types/node-sass": "^4.11.0",
    "@types/prettier": "^1.19.0",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-modal": "^3.10.0",
    "@types/react-test-renderer": "^16.9.1",
    "@types/rollup-plugin-node-builtins": "^2.1.1",
    "@types/rollup-plugin-postcss": "^2.0.0",
    "@types/sass": "^1.16.0",
    "@types/stylelint": "^9.10.1",
    "@types/webpack": "4.39.8",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "acorn": "6.0.5",
    "auto-changelog": "^1.16.0",
    "awesome-typed-sass-modules": "^1.1.5",
    "babel-jest": "^24.9.0",
    "babel-plugin-emotion": "^10.0.4",
    "babel-preset-gatsby": "^0.2.29",
    "chokidar-cli": "^1.2.2",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "custom-tslint-formatters": "^2.4.0",
    "docz": "^2.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-angellist": "git+https://github.com/venturehacks/eslint-config-angellist#0.2.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-sort-keys-fix": "1.1.0",
    "eslint-plugin-typescript-sort-keys": "0.5.0",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-plugin-sass": "^2.1.28",
    "husky": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "jest-runner-prettier": "^0.3.3",
    "jest-runner-stylelint": "^2.2.1",
    "jscodeshift": "^0.6.4",
    "lint-staged": "^9.3.0",
    "lodash": "^4.17.15",
    "next": "^9.1.4",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "release-it": "^10.4.4",
    "rollup": "^1.21.4",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "6.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "sass": "^1.15.2",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "tslib": "^1.10.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.1.0",
    "typescript": "^3.7.3",
    "typesync": "^0.6.1",
    "webpack": "4.28.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@tippy.js/react": "^2.2.3",
    "camelcase": "^5.3.1",
    "gatsby": "^2.19.18",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-theme-docz": "^2.2.0",
    "gatsby-transformer-sharp": "^2.3.14",
    "react-helmet-async": "^1.0.4",
    "react-spinners": "^0.6.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.11",
    "react": ">= 16.8.6",
    "react-dom": ">= 16.8.6",
    "react-modal": "^3.11.1"
  },
  "resolutions": {
    "get-caller-file": "1.0.3"
  },
  "engines": {
    "node": ">= 8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,graphql,md,markdown}": [
      "prettier --config ./.prettierrc.js --ignore-path ./.prettierignore --write",
      "git add"
    ],
    "**/!(*scss.d).{ts,tsx}": [
      "prettier --config ./.prettierrc.js --ignore-path ./.prettierignore --write",
      "tslint --fix",
      "git add"
    ],
    "**/*.{css,scss}": [
      "prettier --config ./.prettierrc.js --ignore-path ./.prettierignore --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 Chrome major versions",
    "last 2 Firefox major versions",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "ie 11",
    "last 3 Android major versions",
    "last 3 ChromeAndroid major versions",
    "last 2 iOS major versions"
  ]
}
